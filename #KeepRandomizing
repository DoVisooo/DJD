using System;
using System.Security.Cryptography.X509Certificates;

namespace KeepRandomizing
{
    class Program
    {
        static void Main(string[] args)
        {
            Random r = new Random();

            int n = r.Next(20);

            for (int i = 0; i <= n; i++)
            {
                int x = r.Next(500);
                int y = r.Next(500);
                Console.WriteLine("X: " + x + "\nY: " + y + "\n");
            }
        }
    }
}
